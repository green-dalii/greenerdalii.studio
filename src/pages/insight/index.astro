---
import BaseLayout from "../../layouts/BaseLayout.astro";
const allPosts = await Astro.glob("./*.md")
const pageTitle = "V"
---

<BaseLayout>
    <div class="space-y-24 px-8 py-32">
        <ul>
            {allPosts.map((post) => <li><a href={post.url}>{post.frontmatter.title}</a></li>)}
        </ul>
    </div>
</BaseLayout>